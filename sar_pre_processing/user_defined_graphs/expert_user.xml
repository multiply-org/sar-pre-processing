      <displayPosition x="24.0" y="109.0"/>
    </node>
    <node id="Remove-GRD-Border-Noise">
      <displayPosition x="19.0" y="159.0"/>
    </node>
    <node id="Calibration">
      <displayPosition x="95.0" y="219.0"/>
